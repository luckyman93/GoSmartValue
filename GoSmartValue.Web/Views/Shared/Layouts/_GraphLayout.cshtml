@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
@using AV.Common.Entities
@using Microsoft.AspNetCore.Identity

@inject SignInManager<User> SignInManager
@inject UserManager<User> userManager

<!DOCTYPE html>

<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<title>@ViewBag.Title</title>
	<!-- Required meta tags -->
	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
	<meta name="viewport" content="width=device-width" />

	<!-- Material Design for Bootstrap fonts and icons -->
	<link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />

	<!-- Material Design for Bootstrap CSS -->
	<link rel="stylesheet" href="~/css/bootstrap-reboot.min.css" />
	<link rel="stylesheet" href="~/css/mdb.min.css" />
	<link rel="stylesheet" href="~/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/bootstrap-grid.min.css" />
	<link rel="stylesheet" href="~/css/bootstrap-select.css" />
	<link rel="stylesheet" href="~/css/datatables.css" />

	<link rel="stylesheet" href="~/css/font-awesome.css" />
	<link rel="stylesheet" href="~/css/site.min.css" />

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js">
	</script>
	<script defer
			src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBU4E8N__WibrMHt17jc0XH1pG9kEboJgk&callback=initMap"></script>
		<script type="text/javascript" src="/js/googlecharts.min.js"></script>

	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-166777042-1"></script>
	<script>
				window.dataLayer = window.dataLayer || [];
				function gtag() {dataLayer.push(arguments); }
				gtag('js', new Date());

				gtag('config', 'UA-166777042-1');
	</script>
	<script>
				window.fwSettings = {
					'widget_id': 66000001161
				};
				!function () { if ("function" != typeof window.FreshworksWidget) { var n = function () {n.q.push(arguments)}; n.q = [], window.FreshworksWidget = n } }()
	</script>
	<script type='text/javascript' src='https://widget.freshworks.com/widgets/66000001161.js' async defer></script>
	@Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body>
	<div class="container-fluid page-wrapper" style="margin:0px;padding:0px;">
		@*<%--header--%>*@
		<partial name="_LoginPartial" />


		<div>
			@RenderBody()
		</div>
	</div>

	<div class="modal fade bd-example-modal-sm" id="swiffycontainer" tabindex="-1" role="dialog" aria-labelledby="swiffycontainer" aria-hidden="true">
		<div class="modal-dialog modal-sm">
			<div class="modal-content p-2">
				<div class="container-fluid text-center"><img height="30" width="30" src="~/big-progress.gif" /></div>
				<div class="container-fluid text-center">
					<p>Loading Please Wait...</p>
				</div>
			</div>
		</div>
	</div>

	<script>
		function ShowProgress() {
			$('#swiffycontainer').modal('show');
		}

		function HideProgress() {
			$('#swiffycontainer').modal('hide');
		}
	</script>

	<script src="~/lib/jquery-validate/jquery.validate.min.js" type="text/javascript"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>

	<script src="~/js/bootstrap-bundle.min.js"></script>
	<script src="~/lib/jquery-ui/jquery-ui.js"></script>
	<script src="~/js/font-awesome.min.js"></script>
	<script src="~/lib/bootstrap-material/popper.min.js" asp-append-version="true"></script>
	<script src="~/lib/bootstrap-material/mdb.min.js"></script>

	<script src="~/js/site.min.js" asp-append-version="true">
	</script>

	@RenderSection("Scripts", required: false)

</body>
</html>
